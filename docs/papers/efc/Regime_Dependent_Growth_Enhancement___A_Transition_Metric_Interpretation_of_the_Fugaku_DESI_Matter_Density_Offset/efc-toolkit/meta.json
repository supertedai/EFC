  "equations": {
    "effective_coupling": {
      "latex": "\\mu(a) \\equiv \\frac{G_{\\mathrm{eff}}(a)}{G}",
      "description": "Dimensionless effective gravitational coupling"
    },
    "transition_kernel": {
      "latex": "\\mu(a) = 1 + \\Delta\\mu \\, \\mathcal{R}\\!\\left(\\frac{\\ln a - \\ln a_t}{\\sigma}\\right)",
      "description": "Smooth transition function",
      "parameters": {
        "delta_mu": "Enhancement amplitude",
        "a_t": "Transition epoch",
        "sigma": "Transition width",
        "R": "Monotonic regulator (sigmoid/tanh)"
      }
    },
    "transition_estimator": {
      "latex": "\\Delta_{\\mathrm{F}} \\equiv \\int W(a)\\,[\\mu(a)-1]\\,d\\ln a",
      "description": "Fugaku/DESI transition metric",
      "reference_value": 0.1,
      "uncertainty": 0.03
    }
  },
  "consistency_conditions": [
    {
      "name": "CMB Consistency",
      "requirement": "μ(a) → 1 for a ≪ a_t",
