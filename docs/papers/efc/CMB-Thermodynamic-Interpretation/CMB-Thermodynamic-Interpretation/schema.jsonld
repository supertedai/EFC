  "efc:causalStructure": {
    "@type": "efc:CausalChain",
    "efc:steps": [
      {
        "@type": "efc:ObservationalConstraint",
        "name": "JWST/COSMOS-Web",
        "efc:constrains": "S(z)"
      },
      {
        "@type": "efc:EntropyProfile",
        "name": "S(z)",
        "efc:derivedFrom": "JWST",
        "efc:testedAgainst": "CMB"
      },
      {
        "@type": "efc:CompatibilityTest",
        "name": "CMB Observables",
        "efc:parameterFitting": false
      }
    ]
  },
  "efc:compatibilityConstraints": [
    {
      "@type": "efc:Constraint",
      "name": "Acoustic Scale",
      "efc:symbol": "theta_s",
      "efc:value": "100.09 +/- 0.30 arcmin",
      "efc:type": "must_hold"
    },
    {
      "@type": "efc:Constraint",
      "name": "Peak Ratio",
      "efc:symbol": "R",
      "efc:value": "~0.42",
      "efc:type": "must_hold"
    },
    {
      "@type": "efc:Constraint",
      "name": "Damping Scale",
      "efc:symbol": "l_D",
      "efc:value": "Silk damping form",
      "efc:type": "must_hold"
    },
    {
      "@type": "efc:Constraint",
      "name": "TE/EE Correlation",
      "efc:value": "positive",
      "efc:type": "must_hold"
    }
  ],
  "efc:predictions": [
    {
      "@type": "efc:Prediction",
      "name": "Lensing Amplitude",
      "efc:symbol": "A_L",
      "efc:prediction": "< 1.0",
      "efc:falsification": "A_L >= 1.0"
    },
    {
      "@type": "efc:Prediction",
      "name": "ISW Phase",
      "efc:prediction": "0.01-0.1 rad at l > 1000"
    },
    {
      "@type": "efc:Prediction",
      "name": "Lensing Spectrum",
      "efc:symbol": "C_l^phiphi",
      "efc:prediction": "z-dependent at high l"
    }
  ],
  "efc:nullTests": [
    {
      "@type": "efc:NullTest",
      "name": "Peak Position Invariance",
      "efc:criterion": "theta_s within 0.5%",
      "efc:falsifiable": true
    },
    {
      "@type": "efc:NullTest",
      "name": "Damping Consistency",
      "efc:criterion": "l_D within 5%",
      "efc:falsifiable": true
    },
    {
      "@type": "efc:NullTest",
      "name": "Lensing-Growth Consistency",
      "efc:criterion": "A_L in 0.9-0.95",
      "efc:falsifiable": true
    }
  ],
  "efc:parameters": [
    {
      "@type": "efc:Parameter",
      "name": "S_0",
      "description": "Entropy at z=0",
      "efc:constraintSource": "SPARC rotation curves"
    },
    {
      "@type": "efc:Parameter",
      "name": "S_eq",
      "description": "Entropy at matter-radiation equality",
      "efc:constraintSource": "CMB peak structure"
    },
    {
      "@type": "efc:Parameter",
      "name": "alpha_S",
      "description": "Entropy coupling strength",
      "efc:constraintSource": "JWST sSFR correlation"
    },
    {
      "@type": "efc:Parameter",
      "name": "n_S",
      "description": "Entropy profile exponent",
      "efc:constraintSource": "Cross-scale consistency"
    }
  ],
