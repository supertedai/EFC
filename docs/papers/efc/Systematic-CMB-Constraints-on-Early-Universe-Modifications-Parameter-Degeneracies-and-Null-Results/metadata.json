  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/efc-cmb-testing"
  },
  "data": {
    "source": "Planck 2018",
    "spectrum": "Temperature (TT)",
    "points": 2507,
    "ell_range": [2, 2508]
  },
  "methods": {
    "boltzmann_solver": "CLASS v3.2.0",
    "analysis": "Python 3.12",
    "statistics": "Amplitude marginalization + chi-squared"
  },
  "results": {
    "mechanism_1": {
      "name": "Thomson scattering (kappa-dot)",
      "type": "microscopic",
      "status": "rejected",
      "minimum": "boost = 0%",
      "delta_chi2": "+5.4 at +/-1%"
    },
    "mechanism_2": {
      "name": "Background energy density H(z)",
      "type": "macroscopic",
      "status": "rejected (after 2D analysis)",
      "minimum": "epsilon approximately 0%",
      "delta_chi2_1d": "-65 at epsilon=+3% (artifact)",
      "delta_chi2_2d": "-1.26 at epsilon=-0.5% (noise)"
    },
    "key_finding": "Parameter degeneracy between epsilon and h created false signal in 1D scan"
  },
